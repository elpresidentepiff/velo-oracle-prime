# VÉLØ v9.0++ CHAREX - Environment Variables
# Copy this file to .env and fill in your credentials

# ============================================================================
# BETFAIR API CREDENTIALS (Oracle 2.0)
# ============================================================================
BETFAIR_USERNAME=your_betfair_username
BETFAIR_PASSWORD=your_betfair_password
BETFAIR_APP_KEY=your_betfair_app_key

# ============================================================================
# THE RACING API CREDENTIALS (Oracle 2.0)
# ============================================================================
RACING_API_USERNAME=your_racing_api_username
RACING_API_PASSWORD=your_racing_api_password
RACING_API_BASE_URL=https://api.theracingapi.com/v1

# ============================================================================
# DATABASE CONFIGURATION (Oracle 2.0)
# ============================================================================
# PostgreSQL connection string for Oracle 2.0
VELO_DB_CONNECTION=postgresql://velo_user:your_password@localhost:5432/velo_racing

# Legacy MongoDB (v9.0)
MONGO_URI=mongodb://localhost:27017/
MONGO_DB_NAME=velo_oracle

# Supabase (Primary Backend for v12)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_PROJECT_ID=your-project-ref
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here
SUPABASE_ACCESS_TOKEN=your-access-token-here
SUPABASE_DB_URL=postgresql://postgres:[PASSWORD]@db.your-project-ref.supabase.co:5432/postgres

# System Configuration
VELO_VERSION=v9.0++
ENVIRONMENT=development
LOG_LEVEL=INFO

# FastAPI Configuration
API_ENV=production
API_VERSION=v1
LOG_JSON=true
MODEL_REGISTRY_PATH=ml/models/
ACTIVE_MODEL_NAME=SQPE
ACTIVE_MODEL_VERSION=v1_real
CORS_ORIGINS=*

# Agent Configuration
ENABLE_SCOUT=true
ENABLE_ARCHIVIST=true
ENABLE_SYNTH=true

# Analysis Parameters
CONFIDENCE_THRESHOLD=60
MIN_ODDS=3.0
MAX_ODDS=20.0
RESPONSE_TIMEOUT=3

# Genesis Protocol
GENESIS_ALPHA_ENABLED=true
GENESIS_BETA_ENABLED=true
GENESIS_GAMMA_ENABLED=true

