# VÉLØ v9.0++ CHAREX - Environment Variables
# Copy this file to .env and fill in your credentials

# Racing API Credentials
RACING_API_USERNAME=your_username_here
RACING_API_PASSWORD=your_password_here
RACING_API_BASE_URL=https://api.theracingapi.com/v1

# Database Configuration
MONGO_URI=mongodb://localhost:27017/
MONGO_DB_NAME=velo_oracle

# Supabase (Alternative Storage)
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key

# System Configuration
VELO_VERSION=v9.0++
ENVIRONMENT=development
LOG_LEVEL=INFO

# Agent Configuration
ENABLE_SCOUT=true
ENABLE_ARCHIVIST=true
ENABLE_SYNTH=true

# Analysis Parameters
CONFIDENCE_THRESHOLD=60
MIN_ODDS=3.0
MAX_ODDS=20.0
RESPONSE_TIMEOUT=3

# Genesis Protocol
GENESIS_ALPHA_ENABLED=true
GENESIS_BETA_ENABLED=true
GENESIS_GAMMA_ENABLED=true

